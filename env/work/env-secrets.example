# =============================================================================
# WORK ENVIRONMENT SECRETS
# =============================================================================
# Copy this file to 'env-secrets' and add your actual secret values
# This file should NOT be tracked by git

# Company GitHub
export GITHUB_TOKEN="your_work_github_token_here"

# Company API Keys
export COMPANY_API_KEY="your_company_api_key"
export INTERNAL_SERVICE_TOKEN="your_internal_token"

# Database URLs (staging/production access)
export STAGING_DATABASE_URL="postgresql://username:password@staging.company.com/app"

# Work-specific secrets
export WORK_SECRET_KEY_BASE="your_work_secret_key_base"
export JWT_SECRET="your_jwt_secret"

# Company services
export COMPANY_AWS_ACCESS_KEY_ID="your_work_aws_access_key"
export COMPANY_AWS_SECRET_ACCESS_KEY="your_work_aws_secret_key"
export SLACK_BOT_TOKEN="your_slack_bot_token"

# VPN and infrastructure
export VPN_PASSWORD="your_vpn_password"
export KUBERNETES_TOKEN="your_k8s_token"
